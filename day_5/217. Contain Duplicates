class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        unordered_map<int,int> mp;
        int i=0; int j= nums.size()-1;

        while (i<=j){
             mp[nums[i]]+=1;
            if (mp[nums[i]]>1){
                return true;
            }
           
            i++;
        }
        return false;
    }
};
