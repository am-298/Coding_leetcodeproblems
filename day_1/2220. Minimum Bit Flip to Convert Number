class Solution {
public:
    int minBitFlips(int start, int goal) {
        int count=0;
        int x=start, y=goal;
        while ((x!=0)||(y!=0)){
            if ((x&1)!=(y&1)){
                count++;
            }
            x=x>>1;
            y=y>>1;
        }
        return count;
    }
    
};
