class Solution {
public:
    int singleNumber(vector<int>& nums) {
        unordered_map<int,int> mp;
        int res=0;
        for (int i=0; i<nums.size();i++){
            mp[nums[i]]+=1;}
             for (auto &p : mp) {
        if (p.second == 1) {
            res= p.first;
                }
            }
            return res;
    }
};

bit manipulation method :--

class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int ones =0; 
        int twos=0;

        for (int c : nums){
            ones= (ones ^c )& ~(twos);
            twos= (twos ^c) & ~(ones);
        }
        return ones;
    }
};
