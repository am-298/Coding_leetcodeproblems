class Solution {
public:
    int duplicateNumbersXOR(vector<int>& nums) {
        int arr[51];
        int sam=0;
        for (int i=0;i<nums.size();i++){
            arr[nums[i]]+=1;} 
        for (int j=1;j<51;j++){
            if (arr[j]==2){
                sam^=j;
            }
        }
        return sam;

    }
};
